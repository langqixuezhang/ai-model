{
  "name": "my-frist-model",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"src/**/*.{js,vue,json,css,scss,md}\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,json,css,scss,md}\" \"*.{js,json,md}\"",
    "preview": "npm run build && npx serve dist",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "core-js": "^3.8.3",
    "pinia": "^2.3.1",
    "vue": "^3.2.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.0.3",
    "prettier": "^2.8.8"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "semi": [
        "error",
        "never"
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
